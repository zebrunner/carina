#set( $symbol_pound = '#' )
#set( $symbol_dollar = '$' )
#set( $symbol_escape = '\' )
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite verbose="1" name="Carina Demo Tests - Zebrunner Selenium Grid (Chrome DevTools)">

    <parameter name="suiteOwner" value="qpsdemo"/>
    <parameter name="jenkinsJobName" value="ESG-DevTools-Test"/>
    <parameter name="jenkinsJobType" value="web"/>
    <parameter name="jenkinsEnvironments" value="DEMO"/>
    <parameter name="jenkinsEmail" value="demo@zebrunner.com"/>
    <parameter name="jenkinsRegressionPipeline" value="esg_regression"/>
    <parameter name="jenkinsRegressionMatrix_esg_regression" value="browser:chrome"/>

    <test name="ZSG Chrome DevTools Test">
        <classes>
            <class name="${package}.carina.demo.regression.esg.DevToolsTest"/>
        </classes>
    </test>
</suite>
